{
    "name": "befluent/rest-response-formatter",
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/http-foundation": "^5.3",
        "spatie/array-to-xml": "^2.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
